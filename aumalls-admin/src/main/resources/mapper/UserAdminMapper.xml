<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mall.admin.dao.UserAdminDao">

    <select id="selectUserPage" resultType="com.mall.common.domain.User">
        SELECT
            user.user_id,
            user.user_name as userName,
            user.user_address,
            user.user_phone,
            user.user_whitetime
        FROM
            user
    </select>
</mapper>